<div class="page otakantaa" id="page">
  {{> organisms-header }}
  <section role="main" class="main">
    {{> molecules-carousel }}
    <section class="l-two-col">
      <section class="l-main">
        {{#loggedin}}
        {{> molecules-loggedin-lifts }}
        {{/loggedin}}
        {{^loggedin}}
        {{> molecules-anonymous-lifts }}
        {{/loggedin}}
      </section>
      <aside class="l-sidebar">
        <section id="ProjectSearch" class="content-item project-search">
          <div class="helper-container">
            <span class="icon-help" title="Voit etsiä hankkeita asiasanojen ja paikkakuntien mukaan" placement="top">Etsi hankkeita joihin voit osallistua</span>
            <p class="helper-text small is-vishidden">Voit etsiä hankkeita asiasanojen ja paikkakuntien mukaan</p>
          </div>
          <div class="inline-form search-form">
            <ul id="tbHaeHanke" class="tagit-list"></ul>
          </div>
        </section>
        <section id="ExternalRss" class="content-item">
          <div class="semantic-list">
            <h3 class="section-title">
              Ajankohtaista
            </h3>
            <ol>
              <li>
                <a href="http://blogi.otakantaa.fi/blogi/nuorten-osallisuus-verkossa-ja-valtionhallinnossa/" target="_blank">Nuorten osallisuus verkossa ja valtionhallinnossa</a>
                <p class="date">
                  8.4.2014 Blogi.otakantaa.fi
                </p>
              </li>
              <li>
                <a href="http://blogi.otakantaa.fi/blogi/nettiaanestamisen-mahdollisuuksista-keskustellaan-otakantaa-fi-sivustolla/" target="_blank">Nettiäänestämisen mahdollisuuksista keskustellaan otakantaa.fi-sivustolla</a>
                <p class="date">
                  3.4.2014 Blogi.otakantaa.fi
                </p>
              </li>
              <li>
                <a href="http://blogi.otakantaa.fi/blogi/syke-tarkkailijasta-ja-haravasta-demokratian-tyokaluja/" target="_blank">SYKE: Tarkkailijasta ja Haravasta demokratian työkaluja</a>
                <p class="date">
                  4.3.2014 Blogi.otakantaa.fi
                </p>
              </li>
            </ol>
          </div>
        </section>
      </aside>
    </section>
  </section>
  <!--End role=main-->
  {{> organisms-footer }}
</div>
